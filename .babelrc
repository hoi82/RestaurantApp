{
    "env": {
        "development": {
            "plugins": [
                "react-hot-loader/babel"
            ]
        }
    },
    "plugins": [
        "@loadable/babel-plugin"
    ],
    "presets": [        
        [            
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": "last 2 versions",
                    "node": "current"                    
                }
            }
        ],        
        "@babel/preset-react"
    ]
}